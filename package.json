{
  "name": "onsembl-ai",
  "version": "0.1.0",
  "description": "Agent Control Center for orchestrating multiple AI coding agents",
  "private": true,
  "workspaces": [
    "backend",
    "frontend",
    "agent-wrapper",
    "packages/*"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "npm run dev --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --cache",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --cache --fix",
    "lint:workspaces": "npm run lint --workspaces --if-present",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:workspaces": "npm run type-check --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules",
    "clean:all": "npm run clean && rm -rf .eslintcache"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axios": "^1.12.2",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "ws": "^8.18.3"
  },
  "keywords": [
    "ai",
    "agents",
    "control-center",
    "websocket",
    "dashboard",
    "claude",
    "gemini",
    "codex"
  ],
  "author": "Onsembl.ai Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onsembl/onsembl-ai.git"
  },
  "dependencies": {
    "@next/swc-win32-x64-msvc": "^16.0.0",
    "dotenv": "^17.2.2",
    "ioredis": "^5.7.0",
    "lru-cache": "^11.2.1"
  },
  "overrides": {
    "fast-jwt": "^4.0.0"
  }
}
