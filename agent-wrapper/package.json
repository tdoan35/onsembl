{
  "name": "@onsembl/agent-wrapper",
  "version": "0.1.0",
  "description": "Node.js CLI wrappers for AI coding agents",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "onsembl-agent": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "tsc --build",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@onsembl/agent-protocol": "*",
    "@onsembl/command-queue": "*",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.0",
    "jsonwebtoken": "^9.0.2",
    "open": "^9.1.0",
    "ora": "^7.0.0",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.0",
    "strip-ansi": "^7.1.2",
    "ws": "^8.14.0",
    "zod": "^3.22.0"
  },
  "optionalDependencies": {
    "keytar": "^7.9.0",
    "node-pty": "^1.1.0-beta38"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/keytar": "^4.4.0",
    "@types/open": "^6.1.0",
    "@types/ws": "^8.5.0",
    "tsx": "^4.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
